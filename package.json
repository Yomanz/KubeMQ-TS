{
  "name": "kubets",
  "version": "0.0.3",
  "license": "MIT",
  "main": "compiled/index.js",
  "repository": "https://github.com/Yomanz/KubeMQ-ts",
  "author": "Daave <daave@aaathats3as.com>",
  "scripts": {
    "build:proto": "protoc ./kubemq.proto --js_out=\"./generated\" --plugin=\"protoc-gen-ts=../node_modules/.bin/protoc-gen-ts\" --ts_out=\"./generated\"",
    "yea": "protoc-gen-grpc ./kubemq.proto --js_out=\"import_style=commonjs,binary:./generated\" --plugin=\"protoc-gen-grpc,protoc-gen-ts=../node_modules/.bin/protoc-gen-ts\" --ts_out=\"service=grpc-node:./generated\nd\""
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.3",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "google-protobuf": "^3.11.2",
    "grpc": "^1.24.2",
    "protoc-gen-grpc": "^1.3.8",
    "ts-protoc-gen": "^0.12.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.1",
    "nodemon": "^2.0.2",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "files": [
    "compiled/**/*",
    "yarn.lock"
  ]
}
